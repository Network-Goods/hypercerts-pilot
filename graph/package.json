{
  "name": "hypercerts",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "build-goerli": "graph build subgraph-goerli.yaml",
    "deploy-goerli": "graph deploy --product hosted-service bitbeckers/hypercerts-goerli subgraph-goerli.yaml",
    "create-local": "graph create --node http://localhost:8020/ protocol/hypercerts",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 protocol/hypercerts",
    "remove-local": "graph remove --node http://localhost:8020/ protocol/hypercerts",
    "start-local": "docker-compose up -d",
    "stop-local": "docker-compose down --volume",
    "test": "graph codegen && graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.33.0",
    "@graphprotocol/graph-ts": "0.27.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
